{
    "version": 2,
    "builds": [
      {
        "src": "code/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.7.9" }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "code/qr_app/static/(.*)",
        "dest": "code/qr_app/static/$1"
      },
      {
        "src": "code/(.*)",
        "dest": "code/wsgi.py"
      }
    ]
  }